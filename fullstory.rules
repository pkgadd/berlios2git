# fullstory.rules

# acer-apci start
create repository export/fullstory.morgue/acer-acpi.git
end repository

match /acer-acpi/trunk/
	repository export/fullstory.morgue/acer-acpi.git
	branch master
end match

match /acer-acpi/branches/upstream/current/
	repository export/fullstory.morgue/acer-acpi.git
	branch upstream
end match

match /acer-acpi/branches/upstream/([^/]+)/
	repository export/fullstory.morgue/acer-acpi.git
	branch refs/tags/upstream/\1
	substitute branch s/~/_/
	substitute branch s/:/%/
end match

match /acer-acpi/tags/([^/]+)/
	repository export/fullstory.morgue/acer-acpi.git
	branch refs/tags/debian/\1
	substitute branch s/~/_/
	substitute branch s/:/%/
end match
# acer-acpi end


